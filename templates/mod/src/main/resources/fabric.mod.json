{
  "schemaVersion": 1,
  "id": "charmony-example-template",
  "version": "${mod_version}",

  "name": "ExampleName",
  "description": "ExampleDescription.",
  "authors": [
    "svenhjol"
  ],
  "icon": "assets/charmony/textures/gui/sprites/charmony-example-template.png",
  "license": "MIT",
  "environment": "*",
  "mixins": [
    "charmony-example-template.client.mixins.json",
    "charmony-example-template.common.mixins.json"
  ],
  "accessWidener": "charmony-example-template.accesswidener",

  "entrypoints": {
